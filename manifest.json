{
    "name":"Stocktwits Autorepost",
    "version":"1.0",
    "manifest_version":3,
    "content_scripts":[
        {
            "matches":[
                "https://stocktwits.com/"
            ],
            "js":["contentscript.js"]
 
        }
    ],
    "action": {
        "default_popup": "popup.html"
      },
    "permissions": [
        "storage",
        "https://stocktwits.com"
    ]
}